- var theme_customizer      = true;
- var sidebar               = true;
- var scrollbar             = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/taptop
    include ../../components/loader
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-sm-6
                  h3
                    | Avatars
                .col-sm-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')
                        i(data-feather="home")
                    li.breadcrumb-item Ui Kits
                    li.breadcrumb-item.active Avatars
          // Container-fluid starts
          .container-fluid
            .user-profile
              .row
                .col-sm-12.col-xl-6.box-col-6
                  .card
                    .card-header.pb-0
                      h4 Basic & Sizing Avatar
                      span Use 
                        code .bg-light-*
                        | for different color avatar.
                    .card-body
                      .avatar-showcase
                        .avatars
                          div.avatar.bg-light-primary.p-2.rounded-pill
                            img.img-100.rounded-circle(src='../assets/images/user/1.jpg', alt='#') 
                          div.avatar.bg-light-secondary.p-2.rounded-pill
                            img.img-90.rounded-circle(src='../assets/images/user/1.jpg', alt='#')
                          div.avatar.bg-light-success.p-2.rounded-pill
                            img.img-80.rounded-circle(src='../assets/images/user/1.jpg', alt='#')
                          div.avatar.bg-light-danger.p-2.rounded-pill
                            img.img-70.rounded-circle(src='../assets/images/user/1.jpg', alt='#')
                          div.avatar.bg-light-info.p-2.rounded-pill
                            img.img-60.rounded-circle(src='../assets/images/user/1.jpg', alt='#')
                          div.avatar.bg-light-warning.p-2.rounded-pill
                            img.img-50.rounded-circle(src='../assets/images/user/1.jpg', alt='#')
                          div.avatar.bg-light-dark.p-2.rounded-pill
                            img.img-40.rounded-circle(src='../assets/images/user/1.jpg', alt='#')
                .col-sm-12.col-xl-6.box-col-6
                  .card
                    .card-header.pb-0
                      h4 Status Indicator
                      span Use 
                        code .bg-*
                        | for different color status Indicator.
                    .card-body 
                      .avatar-showcase
                        .avatars
                          div.avatar
                            img.img-100.rounded-circle(src='../assets/images/user/3.jpg', alt='#')
                            .status.bg-primary
                          div.avatar
                            img.img-90.rounded-circle(src='../assets/images/user/3.jpg', alt='#')
                            .status.bg-secondary
                          div.avatar
                            img.img-80.rounded-circle(src='../assets/images/user/3.jpg', alt='#')
                            .status.bg-success
                          div.avatar
                            img.img-70.rounded-circle(src='../assets/images/user/3.jpg', alt='#')
                            .status.bg-info
                          div.avatar
                            img.img-60.rounded-circle(src='../assets/images/user/3.jpg', alt='#')
                            .status.bg-danger
                          div.avatar
                            img.img-50.rounded-circle(src='../assets/images/user/3.jpg', alt='#')
                            .status.bg-dark
                          div.avatar
                            img.img-40.rounded-circle(src='../assets/images/user/3.jpg', alt='#')
                            .status.bg-warning
                .col-sm-12.col-xl-6.box-col-6
                  .card
                    .card-header.pb-0
                      h4 Initials
                    .card-body 
                      .avatar-showcase
                        .avatars
                          div.avatar
                            img.img-100.rounded-circle(src='../assets/images/user/11.png', alt='#') 
                          div.avatar
                            img.img-90.rounded-circle(src='../assets/images/user/12.png', alt='#')
                          div.avatar
                            img.img-80.rounded-circle(src='../assets/images/user/14.png', alt='#')
                          div.avatar
                            img.img-70.rounded-circle(src='../assets/images/user/2.png', alt='#')
                          div.avatar
                            img.img-60.rounded-circle(src='../assets/images/user/3.png', alt='#')
                          div.avatar
                            img.img-50.rounded-circle(src='../assets/images/user/12.png', alt='#')
                          div.avatar
                            img.img-40.rounded-circle(src='../assets/images/user/10.jpg', alt='#')
                .col-sm-12.col-xl-6.box-col-6
                  .card
                    .card-header
                      h4 Groups
                    .card-body 
                      .avatar-showcase
                        .customers.d-inline-block.avatar-group
                          ul
                            li.d-inline-block
                              img.img-70.rounded-circle(src='../assets/images/user/11.png', alt='')
                            li.d-inline-block
                              img.img-70.rounded-circle(src='../assets/images/user/12.png', alt='')
                            li.d-inline-block
                              img.img-70.rounded-circle(src='../assets/images/user/14.png', alt='')
                        .customers.d-inline-block.avatar-group
                          ul
                            li.d-inline-block
                              img.img-50.rounded-circle(src='../assets/images/dashboard/default/05.png', alt='')
                            li.d-inline-block
                              img.img-50.rounded-circle(src='../assets/images/dashboard/default/02.png', alt='')
                            li.d-inline-block
                              img.img-50.rounded-circle(src='../assets/images/dashboard/default/03.png', alt='')
                        .customers.d-inline-block.avatar-group
                          ul
                            li.d-inline-block
                              img.img-40.rounded-circle(src='../assets/images/user/3.jpg', alt='')
                            li.d-inline-block
                              img.img-40.rounded-circle(src='../assets/images/user/5.jpg', alt='')
                            li.d-inline-block
                              img.img-40.rounded-circle(src='../assets/images/user/1.jpg', alt='') 
                .col-sm-12.col-xl-6.box-col-6
                  .card
                    .card-header.pb-0
                      h4 Shape
                    .card-body 
                      .avatar-showcase
                        .avatars
                          div.avatar
                            img.img-100.b-r-8(src='../assets/images/user/6.jpg', alt='#') 
                          div.avatar
                            img.img-90.b-r-30(src='../assets/images/user/6.jpg', alt='#')
                          div.avatar
                            img.img-80.b-r-35(src='../assets/images/user/6.jpg', alt='#')
                          div.avatar
                            img.img-70.rounded-circle(src='../assets/images/user/6.jpg', alt='#')
                          div.avatar
                            img.img-60.b-r-25(src='../assets/images/user/6.jpg', alt='#')
                          div.avatar
                            img.img-50.b-r-15(src='../assets/images/user/6.jpg', alt='#')
                          div.avatar
                            img.img-40.b-r-10(src='../assets/images/user/6.jpg', alt='#')
                .col-sm-12.col-xl-6.box-col-6
                  .card
                    .card-header.pb-0
                      h4 Ratio
                    .card-body 
                      .avatar-showcase
                        .avatars
                          div.avatar.ratio
                            img.b-r-8.img-100(src='../assets/images/user/7.jpg', alt='#') 
                          div.avatar.ratio
                            img.b-r-8.img-90(src='../assets/images/user/7.jpg', alt='#')
                          div.avatar.ratio
                            img.b-r-8.img-80(src='../assets/images/user/7.jpg', alt='#')
                          div.avatar.ratio
                            img.b-r-8.img-70(src='../assets/images/user/7.jpg', alt='#')
                          div.avatar.ratio
                            img.b-r-8.img-60(src='../assets/images/user/7.jpg', alt='#')
                          div.avatar.ratio
                            img.b-r-8.img-50(src='../assets/images/user/7.jpg', alt='#')
                          div.avatar.ratio
                            img.b-r-8.img-40(src='../assets/images/user/7.jpg', alt='#')
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
